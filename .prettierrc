{
    "parser": "vue",
    "printWidth": 200,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "vueIndentScriptAndStyle": false,
    "arrowParens": "always",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "overrides": [
        {
            "files": "*.js",
            "options": {
                "parser": "babel"
            }
        },
        {
            "files": ["*.json", ".prettierrc"],
            "options": { "parser": "json" }
        },
        {
            "files": ["*.ts"],
            "options": { "parser": "typescript" }
        }
    ]
}
